<div class="text-success">
  <%= flash[:notice] %>
</div>

<h1>ユーザー一覧画面</h1>

<%= render '/books/infocreate', book: @book %>

<% @users.each do |user| %>
  <h4>ユーザー画像</h4>
  <%= image_tag user.get_profile_image(100,100) %>
  <h4>name</h4>
  <%= user.name %>
  <%= link_to "Show", user_path(user.id) %>
<% end %>